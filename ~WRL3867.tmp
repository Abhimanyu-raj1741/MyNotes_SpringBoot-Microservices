\par \hich\af31506\dbch\af31505\loch\f31506 @FeignClient(name = "WELCOME-API")
\par \hich\af31506\dbch\af31505\loch\f31506 public interface WelcomeApiClient \{
\par \tab 
\par \tab \hich\af31506\dbch\af31505\loch\f31506 @GetMapping("/welcome")
\par \tab \hich\af31506\dbch\af31505\loch\f31506 public String invokeWelcomeMsg();
\par 
\par \}
