\par \hich\af31506\dbch\af31505\loch\f31506 => Inject feign client into GreetRestController like below
\par 
\par \hich\af31506\dbch\af31505\loch\f31506 @RestController
\par \hich\af31506\dbch\af31505\loch\f31506 public class GreetRestController \{
\par \tab 
\par \tab \hich\af31506\dbch\af31505\loch\f31506 @Autowired
\par \tab \hich\af31506\dbch\af31505\loch\f31506 private WelcomeApiClient welcomeClient;
\par \tab 
\par \tab \hich\af31506\dbch\af31505\loch\f31506 @GetMapping("/greet")
\par \tab \hich\af31506\dbch\af31505\loch\f31506 public String getGreetMsg() \{
\par \tab \tab 
\par \tab \tab \hich\af31506\dbch\af31505\loch\f31506 String welcomeMsg = welcomeClient.invokeWelcomeMsg();
\par \tab \tab 
